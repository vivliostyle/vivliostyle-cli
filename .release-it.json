{
  "git": {
    "commitMessage": "chore: release v${version}",
    "pushArgs": ["--no-verify"],
    "tagName": "v${version}"
  },
  "github": {
    "release": true
  },
  "npm": {
    "skipChecks": true
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": "angular",
      "infile": "CHANGELOG.md"
    }
  }
}
