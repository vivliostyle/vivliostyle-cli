exports[`generate files with multiple cover pages 1`] = `
"
:root {
  break-before: recto;
}
@page :nth(1) {
  --vs-document-first-page-counter-reset: page 0;
  counter-reset: var(--vs-document-first-page-counter-reset);
}
"
`;

exports[`generate files with multiple cover pages 2`] = `
"
:root {
  break-before: left;
}
body {
  margin: 0;
}
[role=\\"doc-cover\\"] {
  display: block;
  width: 100vw;
  height: 100vh;
  object-fit: contain;
}
@page {
  margin: 0;
}
"
`;
